generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI")
}

model User {
  id          String   @id @default(uuid())
  login       String   @unique
  email       String   @unique
  password    String
  age         Int
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}